<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->
    <head>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
        <meta http-equiv="content-type" content="text/html;charset=utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>{% block title %}{% endblock %} - Celsius3</title>
        <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />
        {% block stylesheets %}
            {% stylesheets
                        'bundles/celsius3core/components/bootstrap/dist/css/bootstrap.min.css'
                        'bundles/celsius3core/components/fontawesome/css/font-awesome.min.css'
                        'bundles/celsius3core/components/select2-ng/select2.css'
                        'bundles/celsius3core/components/datatables/media/css/dataTables.bootstrap.min.css'
                        'bundles/celsius3core/components/select2-bootstrap-css/select2-bootstrap.min.css'
                        'bundles/celsius3core/components/angular-loading-bar/build/loading-bar.min.css'
                        'bundles/celsius3core/components/summernote/dist/summernote.css'
                        'bundles/celsius3core/components/c3/c3.min.css'
                        'bundles/celsius3core/components/bootstrap-switch/dist/css/bootstrap3/bootstrap-switch.min.css'
                        'bundles/celsius3core/components/jqueryui-timepicker-addon/dist/jquery-ui-timepicker-addon.min.css'
                        'bundles/celsius3core/components/jqueryui/themes/smoothness/jquery-ui.min.css'
                        '@Celsius3CoreBundle/Resources/public/less/*' filter='?uglifycss' filter='cssrewrite' %}
            <link rel="stylesheet" type="text/css" href="{{ asset_url }}" />
            {% endstylesheets %}
            {% if instance.get('instance_css').value is not null %}
                <style type="text/css">
                    {{  instance.get('instance_css').value  }}
                </style>
            {% endif %}
            <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
            <!--[if lt IE 9]>
                <script type="text/javascript" src="{{ asset('bundles/celsius3core/js/html5shiv.js') }}"></script>
                <script type="text/javascript" src="{{ asset('bundles/celsius3core/js/respond.min.js') }}"></script>
            <![endif]-->
        {% endblock %}
        {% if app.environment == 'prod' %}
        <script>
            (function (i, s, o, g, r, a, m) {
                i['GoogleAnalyticsObject'] = r;
                i[r] = i[r] || function () {
                    (i[r].q = i[r].q || []).push(arguments)
                }, i[r].l = 1 * new Date();
                a = s.createElement(o),
                        m = s.getElementsByTagName(o)[0];
                a.async = 1;
                a.src = g;
                m.parentNode.insertBefore(a, m)
            })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

            ga('create', 'UA-15165518-47', 'auto');
            ga('send', 'pageview');
        </script>
        {% endif %}
    </head>
    <body>
        <!--[if lt IE 7]>
            <p class="chromeframe">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> or <a href="http://www.google.com/chromeframe/?redirect=true">activate Google Chrome Frame</a> to improve your experience.</p>
        <![endif]-->
        {% block header %}
            <div id="header">
                <nav class="navbar navbar-default navbar-fixed-top navbar-inverse" role="navigation">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="{{ path('directory_homepage') }}">Celsius3</a>
                    </div>
                    <div class="collapse navbar-collapse navbar-ex1-collapse">
                        {% block navigation_top %}
                            {% include 'Celsius3CoreBundle:Layout:_navigation.html.twig' %}
                        {% endblock %}
                        {% block instance_panels %}
                            {% include 'Celsius3CoreBundle:Layout:_user_panel.html.twig' %}
                            {% include 'Celsius3CoreBundle:Layout:_instances_panel.html.twig' %}
                            {% include 'Celsius3MessageBundle:Layout:_message_panel.html.twig' %}
                            {% include 'Celsius3NotificationBundle:Layout:_notification_panel.html.twig' %}
                        {% endblock %}
                        {% include 'Celsius3CoreBundle:Layout:_lang_panel.html.twig' %}
                        {% include 'Celsius3CoreBundle:Layout:_activity.html.twig' %}
                    </div>
                </nav>
            </div>
        {% endblock %}
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    {% block body_title %}
                        <div class="row">
                            <div class="jumbotron page-header clearfix">
                                <div class="col-xs-12 col-sm-12 instance-info">
                                    <div class="row">
                                        <div class="col-xs-3 col-sm-3">
                                            {% if instance.get('instance_logo').value != '' %}
                                                <img src="/uploads/logos/{{ instance.get('instance_logo').value }}" alt="Instance logo" class="instance-logo" />
                                            {% else %}
                                                <img src="{{ asset('bundles/celsius3core/images/celsius200x100.png') }}" alt="Instance logo" class="instance-logo" />
                                            {% endif %}
                                        </div>
                                        <div class="col-xs-9 col-sm-9">
                                            <h2 class="instance-title" >{{ instance.get('instance_title').value }}</h2>
                                            <h3 class="instance-tagline">{% if instance.get('instance_tagline') %}{{ instance.get('instance_tagline').value }}{% endif %}</h3>
                                        </div>
                                    </div>
                                    <div class="row" style="margin-top: 10px; margin-bottom: -22px; margin-left: -20px; margin-right: -20px;">
                                    {% block adminpanel %}{% endblock %}
                                </div>
                            </div>
                        </div>
                    </div>

                    {% if( is_granted('ROLE_SUPERADMIN') or is_granted('ROLE_ADMIN')) %}
                        {% for label, flashes in app.session.flashbag.get('configuration') %}
                            {% for flash in flashes %}
                                <div class="alert alert-warning">
                                    {{ flash }}
                                </div>
                            {% endfor %}
                        {% endfor %}
                    {% endif %}
                {% endblock %}
            {% block navigation_content %}{% endblock %}
        {% block content %}{% endblock %}
    </div>
</div>
<div class="row">
    {% block footer %}
        <footer class="footer">
            <p>{{ 'DevelopedBy' | trans  }} <a href="http://prebi.unlp.edu.ar">PREBI</a>-<a href="http://sedici.unlp.edu.ar">SEDICI</a>
                <a href="http://www.unlp.edu.ar">Universidad Nacional de La Plata</a> <span class="copyleft">&copy;</span> Copyleft {{ date()|date('Y') }} </p>
        </footer>
    {% endblock %}
</div>
</div>
{% include 'Celsius3CoreBundle:Layout:_mail_modal.html.twig' %}
{% block javascripts %}
    {% include 'Celsius3CoreBundle:Layout:var.html.twig' %}
    {% include 'Celsius3NotificationBundle:Layout:var.html.twig' %}
    <script type="text/javascript" src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script type="text/javascript" src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
    {% javascripts
                            'bundles/celsius3core/components/jquery/dist/jquery.min.js'
                            'bundles/celsius3core/components/jqueryui/jquery-ui.min.js'
                            'bundles/celsius3core/components/jqueryui-timepicker-addon/dist/jquery-ui-timepicker-addon.min.js'
                            'bundles/celsius3core/components/bootstrap/dist/js/bootstrap.min.js'
                            'bundles/celsius3core/components/jQuery.dotdotdot/src/js/jquery.dotdotdot.min.js'
                            'bundles/celsius3core/components/icanhaz/ICanHaz.min.js'
                            'bundles/celsius3core/components/underscore/underscore-min.js'
                            'bundles/celsius3core/components/select2-ng/select2.min.js'
                            'bundles/celsius3core/components/datatables/media/js/jquery.dataTables.min.js'
                            'bundles/celsius3core/components/datatables/media/js/dataTables.bootstrap.min.js'
                            'bundles/celsius3core/components/angular/angular.min.js'
                            'bundles/celsius3core/components/angular-route/angular-route.min.js'
                            'bundles/celsius3core/components/angular-resource/angular-resource.min.js'
                            'bundles/celsius3core/components/angular-animate/angular-animate.min.js'
                            'bundles/celsius3core/components/angular-ui-sortable/sortable.min.js'
                            'bundles/celsius3core/components/angular-ui-select2/src/select2.js'
                            'bundles/celsius3core/components/angular-strap/dist/angular-strap.min.js'
                            'bundles/celsius3core/components/angular-strap/dist/angular-strap.tpl.min.js'
                            'bundles/celsius3core/components/fabric/dist/fabric.min.js'
                            'bundles/celsius3core/components/angular-sanitize/angular-sanitize.min.js'
                            'bundles/celsius3core/components/angular-loading-bar/build/loading-bar.min.js'
                            'bundles/celsius3core/components/angular-translate/angular-translate.min.js'
                            'bundles/celsius3core/components/angular-translate-loader-static-files/angular-translate-loader-static-files.min.js'
                            'bundles/celsius3core/components/summernote/dist/summernote.min.js'
                            'bundles/celsius3core/components/angular-bootstrap/ui-bootstrap-tpls.min.js'
                            'bundles/celsius3core/components/d3/d3.min.js'
                            'bundles/celsius3core/components/c3/c3.min.js'
                            'bundles/celsius3core/components/ng-file-upload-shim/ng-file-upload-shim.min.js'
                            'bundles/celsius3core/components/ng-file-upload/ng-file-upload.min.js'
                           'bundles/celsius3core/components/bootstrap-switch/dist/js/bootstrap-switch.min.js'
                           'bundles/celsius3core/components/remarkable-bootstrap-notify/bootstrap-notify.min.js'
                           'bundles/celsius3core/js/jquery-ui-timepicker-addon.js'
                           'bundles/celsius3core/js/cosmetics.js'
                           'bundles/celsius3core/js/forms.js'
                           'bundles/celsius3core/js/cci.js'
                           'bundles/celsius3core/js/admin.js'
                           'bundles/celsius3core/js/dnd.js'
                           'bundles/celsius3core/js/mail.js'
                           'bundles/celsius3notification/js/autobahn.min.js'
                           'bundles/celsius3notification/js/notification.js' filter='?uglifyjs2'  %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
    {% include 'Celsius3CoreBundle:Layout:locales.html.twig' %}
    <!--[if lte IE 8]><script language="javascript" type="text/javascript" src="{{ asset('bundles/celsius3core/js/excanvas.min.js') }}"></script><![endif]-->
    {% include 'Celsius3CoreBundle:Layout:cci.html.twig' %}
    {% include 'Celsius3CoreBundle:Layout:table.html.twig' %}
    {% include 'Celsius3CoreBundle:Layout:mail.html.twig' %}
    {% include 'Celsius3NotificationBundle:Layout:notification.html.twig' %}
{% endblock %}
</body>
</html>
