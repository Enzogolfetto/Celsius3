<dl class="dl-horizontal">
    <dt>{{ 'Name'|trans }}</dt>
    <dd>{{ element.name }}</dd>
    <dt>{{ 'Surname'|trans }}</dt>
    <dd>{{ element.surname }}</dd>
    <dt>{{ 'Birthdate'|trans }}</dt>
    <dd>{{ element.birthdate|localizeddate('medium','none') }}</dd>
    <dt>{{ 'Username'|trans }}</dt>
    <dd>{{ element.username }}</dd>
    <dt>{{ 'Email'|trans }}</dt>
    <dd>{{ element.email }}</dd>
    <dt>{{ 'Address'|trans }}</dt>
    <dd>{{ element.address }}</dd>
    <dt>{{ 'Institution'|trans }}</dt>
    <dd>{{ element.institution?element.institution:'-' }}</dd>
    {% block extrainfo %}{% endblock %}
</dl>