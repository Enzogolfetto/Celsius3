{% extends 'CelsiusCelsius3Bundle:MaterialType:_materialType.html.twig' %}

{% block extraData %}
<dt>{{ 'Editor'|trans }}</dt>
<dd>{{ materialData.editor?materialData.editor:'-' }}</dd>
<dt>{{ 'Chapter'|trans }}</dt>
<dd>{{ materialData.chapter?materialData.chapter:'-' }}</dd>
<dt>{{ 'ISBN'|trans }}</dt>
<dd>{{ materialData.ISBN?materialData.ISBN:'-' }}</dd>
<dt>{{ 'With Index'|trans }}</dt>
<dd>{{ (materialData.withIndex?'Yes':'No')|trans }}</dd>
{% endblock %}