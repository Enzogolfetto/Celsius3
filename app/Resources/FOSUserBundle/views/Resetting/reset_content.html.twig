{% form_theme form theme %}
<form action="{{ path('fos_user_resetting_reset', {'token': token}) }}" {{ form_enctype(form) }} method="POST" class="fos_user_resetting_reset well">
    {{ form_widget(form) }}
    <div>
        <button type="submit" class="btn btn-primary"><i class="icon-ok"></i> {{ 'resetting.reset.submit'|trans({}, 'FOSUserBundle') }}</button>
    </div>
</form>
